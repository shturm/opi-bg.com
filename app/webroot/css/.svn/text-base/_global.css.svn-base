 font-weight:bold;
 padding-bottom:70px;
 
}

#inner-tabs div.tab-text
{ 
 	-moz-transform:rotate(-90deg); 
	-moz-transform-origin: 0 0;
	
	-webkit-transform: rotate(-90deg);
	-webkit-transform-origin: 0 0;
	
	-o-transform: rotate(-90deg);
	-o-transform-origin:  0 0;
 width:95px;
 height:30px;
 background-image:url('../images/tab_image.png');
 background-repeat:no-repeat;
 text-align:center;
 float:left;
}


.tab-content
{
  color:white;
  position:absolute;
  margin-left:30px;
  margin-top:-95px;
  float:left;
  background-image:url('../images/bg_tab.png');
  z-index:100;
  min-width:500px;
  min-height:400px;
  display:none;
}
