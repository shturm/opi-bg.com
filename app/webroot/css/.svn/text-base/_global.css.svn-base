.dropdown-menu
{
 position:absolute;
 margin-top:6px;
 background-image:url('../images/bg.png');
 display:none;
 z-index:200;
}

.dropdown-menu li:hover
{
 background-color:#808080;
}

.dropdown-menu.level-1
{
 float:right;
 position:absolute;
 margin-left:100%;
 margin-top:-14px;
 background-image:url('../images/bg.png');
 display:none;
 z-index:200;
}

.dropdown-menu ul
{
  list-style:none;
}


.dropdown-menu li
{
 background-image:none;
 white-space:nowrap;
 height:26px;
