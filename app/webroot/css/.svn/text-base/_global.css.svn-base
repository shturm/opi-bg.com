#inner-tabs
{
 height:100%;
 float:right;
 position:absolute;
 right:0;
 bottom:0;
}

#inner-tabs ul
{
 width:30px;
 height:80%;
 list-style:none;
 padding:0;
 margin-top:190px;
 
}
#inner-tabs li
{
 background-image:none;
 float:left;
